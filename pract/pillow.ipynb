   "source": [
    "from PIL import Image\n",
    "import io\n",
    "\n",
    "# Save the image to a BytesIO object\n",
    "buffer = io.BytesIO()\n",
    "# Open the JPEG image\n",
    "with open(\"image.png\", \"rb\") as f:\n",
    "    img = Image.open(f)\n",
    "    img.save(buffer, format=\"PNG\")\n",
    "\n",
    "image_bytes = buffer.getvalue()\n",
    "type(image_bytes)"
   ]
  }
